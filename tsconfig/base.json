{
  "files": [
    "../src/index.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "lib": [
      "ES2019",
      "DOM",
      "ES2018.asynciterable"
    ],
    "types": [],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": false,
    "removeComments": true,
    "verbatimModuleSyntax": false,
    "moduleResolution": "node",
    "baseUrl": "../src",
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
